@{
    ViewBag.Title = "Handing Over Accounts";
    var val = System.Web.Configuration.WebConfigurationManager.AppSettings["SiteVersion"].ToString();
}
@section Scripts {
    <script src="~/Scripts/Common/HandingOverAccounts.js?@val"></script>
}
<div class="op-btns">
    <div class="col-md-6">
        <div class="page-title">Handing Over Accounts</div>
    </div>
    <div class="col-md-6" style="width:540px">
        <input type="button" value="Clear" class="btn btn-default btn-default-style btn-clear-data" />
        <input type="button" value="Save" class="btn btn-default btn-default-style btn-save-data" />
        <input type="button" value="View" class="btn btn-default btn-default-style btn-view-data" />
        <input type="button" value="Excel" class="btn btn-default btn-default-style btn-excel-upload" />
    </div>
</div>
<div class="demopage">
    <div class="container1">
        <div class="panel panel-default row">
            <div class="panel panel-heading"></div>
            <div class="panel-container">
                <div class="panel panel-body tab-content-cls">
                    <div class="form-horizontal">
                        <div class="row">
                            <div class="col-md-3">
                                <label class="col-lg-4 control-label">Date</label>
                                <div class="col-md-8">
                                    <input type="text" id="date" name="fdate" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <label class="col-lg-4 control-label">PC</label>
                                <div class="col-md-8">
                                    <input type="text" id="location" name="location" class="form-control with-search" style="background:#eea7a7">
                                    <img src="~/Resources/Images/Search-icon.png" class="search-img  loc-search">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="col-md-12">
                                    <input type="text" id="locationdesc" name="locationdesc" class="form-control" readonly style="color:#1119b2">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-lg-10 control-label">Details With Balance</label>
                                <input type="checkbox" id="chkop1" name="chkop1" />
                            </div>


                        </div>
                        <div class="row">
                            <div class="col-md-3">
                                <label class="col-lg-4 control-label">Bonus Month</label>
                                <div class="col-md-8">
                                    <input type="text" id="bmonthdate" name="bmonthdate" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <label class="col-lg-4 control-label">Account No</label>
                                <div class="col-md-8">
                                    <input type="text" id="accno" name="accno" class="form-control with-search" >
                                    <img src="~/Resources/Images/Search-icon.png" class="search-img  accno-search">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <label class="col-lg-4 control-label">Reject Amount</label>
                                <div class="col-md-7">
                                    <input type="text" id="rejammount" name="rejammount" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <img src="~/Resources/Images/arrow_down.png" class="search-img add-acc">
                            </div>
                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default row">
        <div class="panel panel-heading">Current Bonus Month </div>
        <div class="panel-container">
            <div class="panel panel-body tab-content-cls">
                <div class="form-horizontal">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="comm-comm-list-div-scroll">
                                <table class="table table-striped table-hover acc-data-row">
                                    <tr>
                                        <th style="color: black; font-size: x-small">Location</th>
                                        <th style="color: black; font-size: x-small">Account</th>
                                        <th style="color: black; font-size: x-small">Bonus Month</th>
                                        <th style="color: black; font-size: x-small; text-align:right">Reject Amount</th>
                                        <th style="color: black; font-size: x-small; text-align: right">Balance</th>
                                        <th></th>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade my-popup-div" id="exclupload" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="container1">
                @using (Html.BeginForm("BindExceldata", "HandingOverAccounts", FormMethod.Post, new { enctype = "multipart/form-data", @id = "imp-data" }))
                {
                    <div class="panel-container">
                        <div id="view1" class="tab-content-cls">
                            <div class="form-horizontal">
                                <div class="row">
                                    <div class="col-md-12 form-group">
                                        <label class="col-md-3 control-label">Upload File</label>
                                        <div class="col-md-3">
                                            <input type="file" name="UploadedFile" id="UploadedFile" class="form-control-file" required />
                                        </div>
                                        <div class="col-md-1">
                                            <input type="button" value="Upload" class="btn btn-xs btn-default imprt-cd-data" />
                                        </div>
                                        <div class="col-md-1">
                                            <input type="button" value="Close" class="btn btn-xs btn-default cls-excel-popup" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                }

            </div>
        </div>
    </div>

</div>

