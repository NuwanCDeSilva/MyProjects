@{
    ViewBag.Title = "Sun Upload";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var val = System.Web.Configuration.WebConfigurationManager.AppSettings["SiteVersion"].ToString();
}
@section Scripts
{
@*<script src="~/Scripts/Common/SunUpload.js"></script>*@
<script src="~/Scripts/Common/SunUpload.js?@val"></script>
}
<div class="content-wrapper">

    <section class="content-header">
        <h1>
            Sun Upload
            <small>Form</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active"> Sun Upload</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content clearfix">
        <!-- button container set begin -->
        <div class="col-md-12 text-right btn-scroll-top ">
            <button class="btn btn-sm btn-ash-fullbg btn-upload">
                <i class="glyphicon glyphicon-pencil"></i> Upload
            </button>

            <button class="btn btn-sm btn-red-fullbg btn-clear">
                <i class="glyphicon glyphicon-refresh"></i> Clear
            </button>
        </div>
        <!-- button container set end-->
        <div class="row">
            <div class="col-lg-8">
                <!-- customer details begin -->
                <div class="box">
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="row">
                            <div class="col-md-12">

                                <div class="col-md-3">
                                    <!-- /.form-group -->
                                    <label>Start Date</label>
                                    <div class="input-group date">
                                        <div class="input-group-addon">
                                            <i class="fa fa-calendar"></i>
                                        </div>
                                        <input type="text" name="stdate" id="stdate" class="form-control pull-right datepic">
                                    </div>
                                    <!-- /.form-group -->
                                </div>
                                <div class="col-md-3">
                                    <!-- /.form-group -->
                                    <label>End Date</label>
                                    <div class="input-group date">
                                        <div class="input-group-addon">
                                            <i class="fa fa-calendar"></i>
                                        </div>
                                        <input type="text" name="enddate" id="enddate" class="form-control pull-right datepic">
                                    </div>
                                    <!-- /.form-group -->
                                </div>

                                @*<div class="col-md-3">
                                    <div class="form-group">
                                        <label>Type</label>
                                        @Html.DropDownList("Type", new List<SelectListItem>(), "Select Type", new { @class = "form-control" })
                                    </div>
                                </div>*@
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label>Divition Type</label>
                                        @Html.DropDownList("DvType", new List<SelectListItem>(), "Select Type", new { @class = "form-control" })
                                    </div>
                                </div>
                                <!-- /.col -->
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label class="checkbox-label">
                                            
                                            <input id="CHEKALL" type="checkbox" value="true" name="CHEKALL" />
                                            Select All
                                        </label>
                                    </div>
                                </div>
                                <!-- /.col -->
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label class="checkbox-label">
                                            <input type="checkbox" class="minimal-red" >
                                            Block
                                        </label>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label>Path</label>   
                                        @Html.Editor("path", new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })                                    
                                    </div>
                                </div>
                                <div class="col-md-9 padding-ls margin-top10">
                                    <table class="table table-bordered table-hover dataTable tbl-pc-list">
                                        <thead>
                                            <tr>
                                                <th>Select</th>
                                                <th>PC</th>
                                            </tr>
                                        </thead>
                                        <tbody class="tbody-scroll300">
                                        </tbody>
                                    </table>
                                </div>

                            </div>
                        </div>
                        <!-- /.row -->
                    </div>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->



            </div>
        </div>

    </section>
    <!-- end main section -->
</div>