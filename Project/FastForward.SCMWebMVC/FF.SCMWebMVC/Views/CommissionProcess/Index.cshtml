@{
    ViewBag.Title = "Commission Process";
    var val = System.Web.Configuration.WebConfigurationManager.AppSettings["SiteVersion"].ToString();
}
@section Scripts {
    <script src="~/Scripts/Common/CommissionProcess.js?@val"></script>
}
<div class="op-btns">
    <div class="col-md-4">
        <div class="page-title">Commission Process</div>
    </div>
    <div class="col-md-8" style="width:800px">
        <input type="button" value="Clear" class="btn btn-default btn-default-style btn-clear-data" />
        <input type="button" value="Elite" class="btn btn-default btn-default-style btn-show-elite-data" />
        <input type="button" value="View" class="btn btn-default btn-default-style btn-show-data" />
        <input type="button" value="Finalize" class="btn btn-default btn-default-style btn-finalize-data" />
        <input type="button" value="Summary" class="btn btn-default btn-default-style btn-viewCommissions-data" />
        <input type="button" value="Detail" class="btn btn-default btn-default-style btn-detail-data" />
        <input type="button" value="With Target" class="btn btn-default btn-default-style btn-target-data" />
        <input type="button" value="Sals Cntrb" class="btn btn-default btn-default-style btn-Sls-Con-data" />
        Excel<input type="checkbox" value="true" checked="checked" id="chkExcel" name="chkExcel" />
       
    </div>
</div>
<div class="demopage">
    <div class="container1">
        <div class="panel panel-default row">
            <div class="panel panel-heading"></div>
            <div class="panel-container">
                <div class="panel panel-body tab-content-cls">
                    <div class="form-horizontal">
                        <div class="row">
                            <div class="col-md-3">
                                <div class="row">
                                    <div class="col-md-10">
                                        <label class="col-lg-4 control-label">Profit Center</label>
                                        <div class="col-md-8">
                                            <input type="text" id="ProfitCenter" name="ProfitCenter" class="form-control with-search">
                                            <img src="~/Resources/Images/Search-icon.png" class="search-img  profit-center-search">
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <img src="~/Resources/Images/arrow_down.png" class="search-img add-pc">
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="comm-pc-div-scroll">
                                            <table class="table table-striped table-hover pc-row">
                                                <tr>
                                                    <th style="color: black; font-size: x-small">PC</th>
                                                    <th style="color: black; font-size: x-small">Option</th>
                                                </tr>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="row">
                                    <div class="col-md-10">
                                        <label class="col-lg-4 control-label">Channel</label>
                                        <div class="col-md-8">
                                            <input type="text" id="chanel" name="chanel" class="form-control with-search">
                                            <img src="~/Resources/Images/Search-icon.png" class="search-img  channel-search">
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-10">
                                        <label class="col-lg-2 control-label">Employee</label>
                                        <label class="col-lg-2 control-label efp-cd"></label>
                                        <div class="col-md-8">
                                            <input type="text" id="overemp" name="overemp" class="form-control with-search">
                                            <img src="~/Resources/Images/Search-icon.png" class="search-img  emp-search">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="col-md-10">
                                    <label class="col-lg-4 control-label">Circular Code</label>
                                    <div class="col-md-8">
                                        <input type="text" id="commcode" name="commcode" class="form-control with-search">
                                        <img src="~/Resources/Images/Search-icon.png" class="search-img  commission-search">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="col-md-10">
                                    <label class="col-lg-4 control-label">From Date</label>
                                    <div class="col-md-8">
                                        <input type="text" id="fdate" name="fdate" class="form-control">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="col-md-10">
                                    <label class="col-lg-4 control-label">To Date</label>
                                    <div class="col-md-8">
                                        <input type="text" id="tdate" name="tdate" class="form-control">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default row">
        <div class="panel panel-heading">Commission Details</div>
        <div class="panel-container">
            <div class="panel panel-body tab-content-cls">
                <div class="form-horizontal">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="comm-comm-list-div-scroll">
                                <table class="table table-striped table-hover commission-data-row">
                                    <tr>
                                        <th style="color: black; font-size: x-small">Exec Code</th>
                                        <th style="color: black; font-size: x-small">Exec Name</th>
                                        <th style="color: black; font-size: x-small">Item Code</th>
                                        <th style="color: black; font-size: x-small">Total Sale</th>
                                        <th style="color: black; font-size: x-small">Total Qty</th>
                                        <th style="color: black; font-size: x-small">Total Commission</th>
                                        <th style="color: black; font-size: x-small">Employee Commission</th>
                                        <th style="color: black; font-size: x-small">Balance Commission</th>
                                        <th style="color: black; font-size: x-small">Invoices</th>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade my-popup-div" id="invoicedetails" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button class="close" data-dismiss="modal">×</button>
                <h4 class="modal-title" id="cus-title">Invoices Details</h4>
            </div>
            <div class="col-md-12">
                <div class="comm-comm-list-div-scroll">
                    <table class="table table-striped table-hover invoice-data-row">
                        <tr>
                            <th style="color: black; font-size: x-small">Invoice No</th>
                            <th style="color: black; font-size: x-small">Item Code</th>
                            <th style="color: black; font-size: x-small">Total Value</th>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

