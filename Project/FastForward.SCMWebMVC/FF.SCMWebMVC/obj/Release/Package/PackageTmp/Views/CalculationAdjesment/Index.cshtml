                <div class="col-md-4 form-group">
                    <label class="col-md-6 control-label">Profit Center</label>
                    <div class="col-md-6">
                        @Html.Editor("pc", new { htmlAttributes = new { @class = "form-control with-search" } })
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-1"></div>
                <div class="col-md-10 form-group item-range-scroll">
