@{
    ViewBag.Title = "Grace Period qualified amount";
    var val = System.Web.Configuration.WebConfigurationManager.AppSettings["SiteVersion"].ToString();
}
@section Scripts {
    <script src="~/Scripts/Common/GPQualifiedAmount.js?@val"></script>
}
<div class="op-btns">
    <div class="col-md-6">
        <div class="page-title">Grace Period Qualified Amount</div>
    </div>
    <div class="col-md-6" style="width:540px">
        <input type="button" value="Clear" class="btn btn-default btn-default-style btn-clear-data" />
        <input type="button" value="Save" class="btn btn-default btn-default-style btn-save-data" />
        <input type="button" value="View" class="btn btn-default btn-default-style btn-view-data" />
    </div>
</div>
<div class="demopage">
    <div class="container1">
        <div class="panel panel-default row">
            <div class="panel panel-default row col-md-6">
                <div class="panel panel-heading">Grace period qualified parameters</div>
                <div class="panel-container">
                    <div class="panel panel-body tab-content-cls">
                        <div class="form-horizontal">
                            <div class="row">
                                <div class="col-md-6">
                                    <label class="col-lg-4 control-label">Circular Code</label>
                                    <div class="col-md-8">
                                        <input type="text" id="circode" name="circode" class="form-control with-search" maxlength="50" >
                                        <img src="~/Resources/Images/Search-icon.png" class="search-img  circular-search">
                                    </div>
                                </div>
                                @*<div class="col-md-6">
