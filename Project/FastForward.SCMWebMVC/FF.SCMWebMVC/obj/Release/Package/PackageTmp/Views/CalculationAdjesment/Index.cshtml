<div class="modal fade my-popup-div" id="pclistpopup" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="cus-title">Manager Profit Centers</h4>
            </div>
