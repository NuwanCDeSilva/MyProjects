@model FF.BusinessObjects.trn_jb_hdr

@{
    ViewBag.Title = "Job Definition";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var val = System.Web.Configuration.WebConfigurationManager.AppSettings["SiteVersion"].ToString();
}
@section Scripts
{
@*<script src="~/Scripts/Common/JobDefinition.js"></script>*@
<script src="~/Scripts/Common/JobDefinition.js?@val"></script>
}

<div class="content-wrapper">

    <section class="content-header">
        <h1>
            Job Definition
            <small>Form</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active"> Job Definition</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content clearfix">
        <!-- button container set begin -->
        <div class="col-md-12 text-right btn-scroll-top ">
            <button class="btn btn-sm btn-green-fullbg btn-save-jobser">
                <i class="glyphicon glyphicon-ok-circle"></i>
                Save
            </button>
            @*<button class="btn btn-sm btn-ash-fullbg">
                <i class="glyphicon glyphicon-pencil"></i> Update
            </button>*@

            <button class="btn btn-sm btn-red-fullbg btn-clr-data">
                <i class="glyphicon glyphicon-refresh"></i> Clear
            </button>
        </div>
@using (Html.BeginForm(null,null, FormMethod.Post, new { enctype = "multipart/form-data", @id = "job-data" }))
{
        <!-- button container set end-->
        <div class="row">
            <div class="col-lg-6">
                <!-- customer details begin -->
                <div class="box">
                    <div class="box-header with-border">
                        <h3 class="box-title form-headding">Job Details</h3>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                        </div>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="row">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Pouch</label>
                                    <div class="input-group input-group-sm">
                                        @*<input type="text" name="pouch" class="form-control">*@
                                        @Html.EditorFor(model => model.Jb_pouch_no, new { htmlAttributes = new { @class = "form-control" } })
                                        <span class="input-group-btn">
                                            <button type="button" data-toggle="modal" data-target="#modalviewdf" class="btn btn-default btn-flat btn-pouch-no"><i class="glyphicon glyphicon-search"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Job</label>
                                    <div class="input-group input-group-sm">
                                        @Html.EditorFor(model => model.Jb_jb_no, new { htmlAttributes = new { @class = "form-control" } })
                                        <span class="input-group-btn">
                                            <button type="button" data-toggle="modal" data-target="#modalviewdf" class="btn btn-default btn-flat search-job-no"><i class="glyphicon glyphicon-search"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-3">
                                 @*/.form-group*@ 
                                <div class="form-group">
                                    <label>Date</label>
                                    <div class="input-group input-group-sm">
                                        @*<div class="input-group-addon">
                <i class="fa fa-calendar"></i>
            </div>*@
                                        @Html.EditorFor(model => model.Jb_jb_dt, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                    </div>
                                    <!-- /.form-group -->
                                </div>
                                </div>

                            @*<div class="col-md-3">
                                <div class="form-group">
                                    <label>Status</label>
                                    @Html.EditorFor(model => model.Jb_stus, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                </div>
                            </div>*@
                            <!-- /.col -->
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Additional Comments</label>
                                    @*<textarea rows="2" name="additional_comment"></textarea>*@
                                    @Html.TextAreaFor(model => model.Jb_rmk, new { rows = 2})
                                </div>
                            </div>
                            <!-- /.col -->
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Console</label>
@Html.RadioButton("console", new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                </div>
                            </div>
                        </div>
                        <!-- /.row -->
                    </div>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->

                <div class="box">
                    <div class="box-header with-border">
                        <h3 class="box-title form-headding">Customer Details</h3>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                        </div>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="row">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Entity Type</label>
                                    <div class="form-group">
                                        @Html.DropDownList("en_type", new List<SelectListItem>(), "Select Type", new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                            <!-- /.col -->

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Code</label>
                                    <div class="input-group input-group-sm">
                                        @Html.Editor("cus_cd", new { htmlAttributes = new { @class = "form-control" } })
                                        <span class="input-group-btn">
                                            <button type="button" data-toggle="modal" data-target="#modalview" class="btn btn-default btn-flat btn-cust_search"><i class="glyphicon glyphicon-search"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <!-- /.col -->
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>TIN Code</label>
                                    <div class="input-group input-group-sm">
                                        @Html.Editor("tin_cd", new { htmlAttributes = new { @class = "form-control" } })
                                        <span class="input-group-btn">
                                            @*<button type="button" data-toggle="modal" data-target="#modalview" class="btn btn-default btn-flat"><i class="glyphicon glyphicon-search"></i></button>*@
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <!-- /.col -->
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Deal Currency</label>
                                    <div class="input-group input-group-sm">
                                        @Html.Editor("dealer_currncy", new { htmlAttributes = new { @class = "form-control" } })
                                        <span class="input-group-btn">
                                            <button type="button" data-toggle="modal" data-target="#modalview" class="btn btn-default btn-flat curency-search"><i class="glyphicon glyphicon-search"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <!-- /.col -->

                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Name</label>
                                    @Html.Editor("cuss_name", new { htmlAttributes = new { @class = "form-control" } })
                                    <label>Address 1</label>
                                    @Html.Editor("cus_addr1", new { htmlAttributes = new { @class = "form-control" } })
                                    <label>Address 2</label>
                                    @Html.Editor("cus_addr2", new { htmlAttributes = new { @class = "form-control" } })
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Sales Executive</label>
                                    <div class="input-group input-group-sm">
                                        @Html.EditorFor(model => model.Jb_sales_ex_cd, new { htmlAttributes = new { @class = "form-control" } })
                                        <span class="input-group-btn">
                                            <button type="button" data-toggle="modal" data-target="#modalview" class="btn btn-default btn-flat btn-exec-search"><i class="glyphicon glyphicon-search"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <!-- /.col -->


                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Country</label>
                                    <div class="input-group input-group-sm">
                                        @Html.Editor("country", new { htmlAttributes = new { @class = "form-control" } })
                                        <span class="input-group-btn">
                                            <button type="button" data-toggle="modal" data-target="#modalview" class="btn btn-default btn-flat cntry-cd-search"><i class="glyphicon glyphicon-search"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <!-- /.col -->

                            <div class="col-md-3">
                                <div class="form-group">
                                    <div class="form-group">
                                        <label>Town</label>
                                        <div class="input-group input-group-sm">
                                            @Html.Editor("town", new { htmlAttributes = new { @class = "form-control" } })
                                            <span class="input-group-btn">
                                                <button type="button" data-toggle="modal" data-target="#modalview" class="btn btn-default btn-flat per-town-search"><i class="glyphicon glyphicon-search"></i></button>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- /.col -->
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Province</label>
                                    <div class="input-group input-group-sm">
                                        @Html.Editor("Province", new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <!-- /.form-group -->
                                <div class="form-group">
                                    <div class="form-group">
                                        <label>Telephone</label>
                                        <div class="input-group input-group-sm">
                                            @Html.Editor("Telephone", new { htmlAttributes = new { @class = "form-control", @maxlength = "12" } })
                                            <span class="input-group-btn">
                                                @*<button type="button" data-toggle="modal" data-target="#modalview" class="btn btn-default btn-flat"><i class="glyphicon glyphicon-search"></i></button>*@
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <!-- /.form-group -->
                            </div>
                            <div class="col-md-3">
                                <!-- /.form-group -->
                                <div class="form-group">
                                    <div class="form-group">
                                        <label>Fax</label>
                                        <div class="input-group input-group-sm">
                                            @Html.Editor("Fax", new { htmlAttributes = new { @class = "form-control" , @maxlength = "12"} })
                                            <span class="input-group-btn">
                                                @*<button type="button" data-toggle="modal" data-target="#modalview" class="btn btn-default btn-flat"><i class="glyphicon glyphicon-search"></i></button>*@
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <!-- /.form-group -->
                            </div>
                            <div class="col-md-6">
                                <!-- /.form-group -->
                                <div class="form-group">
                                    <div class="form-group">
                                        <label>Email</label>
                                        @Html.Editor("email", new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
                                <!-- /.form-group -->
                            </div>
                            <div class="col-md-3">
                                <input type="button" class="btn btn-blue-fullbg btn-sm  add-cus-list" value="Add" />
                            </div>
                        </div>
                        <!-- /.row -->
                    </div>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->


            </div>
            <div class="col-lg-6">
                <!-- contact details begin -->
                <div class="box">
                    <div class="box-header with-border">
                        <h3 class="box-title form-headding">Pending Jobs</h3>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                        </div>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="row">
                            <div class="col-md-12">
                                <table class="table table-bordered table-hover dataTable pending-requset">
                                    <thead>
                                        <tr>
                                            <th>Req No</th>
                                            <th>Perch No</th>
                                            <th>Date</th>
                                            <th>Remark</th>
                                            <th></th>
                                        </tr>
                                    </thead>

                                    <tbody class="tbody-scroll"></tbody>
                                </table>
                            </div>
                        </div>
                        <!-- /.row -->
                    </div>
                    <!-- /.box-body -->
                </div>

                <!-- /.box -->
                <!-- contact details end -->
                <!-- status begin -->
                <div class="box">
                    <div class="box-header with-border">
                        <h3 class="box-title form-headding">Service Details</h3>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                        </div>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="row">
                            <div class="col-md-12">
                                <table class="table table-bordered table-hover dataTable job-services" style="">
                                    <thead>
                                        <tr>
                                            <th>Service Code</th>
                                            <th>Description</th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody class="tbody-scroll" style="height:155px;">
                                    </tbody>
                                </table>

                            </div>
                        </div>
                        <!-- /.row -->
                    </div>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->
                <!-- status begin -->
                <div class="box">
                    <div class="box-header with-border">
                        <h3 class="box-title form-headding"> Details</h3>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                        </div>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="row">
                            <div class="col-md-12">
                                <table class="table table-bordered table-hover dataTable cus-data-list">
                                    <thead>
                                        <tr>
                                            <th>Customer Code</th>
                                            <th>Type</th>
                                            <th>PC</th>
                                            <th>Name</th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                </table>

                            </div>
                        </div>
                        <!-- /.row -->
                    </div>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->

            </div>
        </div>
}

    </section>
    <!-- end main section -->
</div>
<!-- /.content-wrapper -->
    
