@{
    ViewBag.Title = "ChargeCode";
    var val = System.Web.Configuration.WebConfigurationManager.AppSettings["SiteVersion"].ToString();
}
@section Scripts {

    <script src="~/Scripts/Common/ChargeCode.js?@val"></script>
}
<div class="add-emp-cls">
    <div class="container1 op-btns">
        <div class="row">
            <div class="col-md-4">
                <div class="page-title">Package Definition</div>
            </div>
            <div class="col-md-8">
                <input type="button" value="Clear" class="btn btn-default btn-default-style btn-cd-clear-data" />
                <input type="button" value="Save" class="btn btn-default btn-default-style btn-cd-save-data" />
            </div>
        </div>
    </div>
    <div class="demopage">
        <div class="container1">
@using (Html.BeginForm("SaveExcelData", "ChargeCodeDef", FormMethod.Post, new { enctype = "multipart/form-data", @id = "imp-data" }))
{
            <div class="panel-container">
                <div id="view1" class="tab-content-cls">
                    <div class="form-horizontal">
                        <div class="row">
                            <div class="col-md-6 form-group">
                                <label class="col-md-6 control-label">Charge Type</label>
                                <div class="col-md-6">
                                    @Html.DropDownList("ChargeCode", new List<SelectListItem>(), "Select Type", new { @class = "form-control" })
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12 form-group">
                                <label class="col-md-3 control-label">Upload File</label>
                                <div class="col-md-3">
                                    <input type="file" name="UploadedFile" id="UploadedFile" class="form-control-file" required/>
                                </div>
                                <div class="col-md-1">
                                    <input type="button"  value="Submit" class="btn btn-xs btn-default imprt-cd-data" />
                                </div>
                              @if (ViewBag.Alert != null)
                              {
                                <div class="col-md-4 alert alert-success">
                                    <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                                    <strong>@ViewBag.Alert</strong>
                                </div>
                              }
                                    
                              
                            </div>
                        </div>  
                    </div>
                </div>
            </div>
}
@using (Html.BeginForm("SaveAirChageCodes", "ChargeCodeDef", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal air-travel", role = "form", @id = "air-travel" }))
