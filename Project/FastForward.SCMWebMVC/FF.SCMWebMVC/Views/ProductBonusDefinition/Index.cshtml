@{
    ViewBag.Title = "Product Bonus Definition";
    var val = System.Web.Configuration.WebConfigurationManager.AppSettings["SiteVersion"].ToString();
}
@section Scripts {
    <script src="~/Scripts/Common/ProductBonusDefinition.js?@val"></script>
}
<div class="op-btns">
    <div class="col-md-6">
        <div class="page-title">Product Bonus Definition</div>
    </div>
    <div class="col-md-6">
        <input type="button" value="Clear" class="btn btn-default btn-default-style btn-clear-data" />
        <input type="button" value="Save" class="btn btn-default btn-default-style btn-bonus-save-data" />
        <input type="button" value="Excel" class="btn btn-default btn-default-style btn-excel-upload-data" />
        <input type="button" value="Print" class="btn btn-default btn-default-style btn-excel-print-data" />
    </div>
</div>

<div class="demopage">
    <div class="container1">
        <div class="panel panel-default row">
            <div class="panel panel-heading"></div>
            <div class="panel-container">
                <div class="panel panel-body tab-content-cls">
                    <div class="form-horizontal">
                        <div class="col-md-3 row item-range-scroll2">
                            <div class="panel panel-heading">
                                <div class="col-md-6">Company</div>
                                <div class="col-md-6">
                                    @*<div class="col-md-3">@Html.CheckBox("chkmultiple")</div>
                                        <div class="normal-font">Multiple</div>*@
                                </div>
                            </div>
                            <div class="panel panel-body company-tbl">
                                <table class="compny-display-tbl table">
                                    <tr class="table-hed">
                                        <th>@Html.CheckBox("chkAllCompany")</th>
                                        <th>Code</th>
                                        <th>Description</th>
                                    </tr>
                                </table>
                            </div>
                        </div>
                        <div class="col-md-9 row">
                            <div class="col-md-4">
                                <label class="col-lg-3 control-label">Code</label>
                                <div class="col-md-9">
                                    <input type="text" id="bonuscode" name="bonuscode" class="form-control with-search" readonly>
                                    <img src="~/Resources/Images/Search-icon.png" class="search-img  bonus-search">
                                </div>
                            </div>

                            <div class="col-md-4">
                                <label class="col-lg-3 control-label">F/Sale Limit</label>
                                <div class="col-md-9">
                                    <input type="text" id="fwdsalelimit" name="fwdsalelimit" class="form-control">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-9 row">
                            <div class="col-md-4">
                                <label class="col-lg-3 control-label">Areas Limit</label>
                                <div class="col-md-9">
                                    <input type="text" id="areaslimit" name="areaslimit" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <label class="col-lg-3 control-label">Outst Limit</label>
                                <div class="col-md-9">
                                    <input type="text" id="outstanlimit" name="outstanlimit" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <label class="col-lg-3 control-label">O/Dt Limit</label>
                                <div class="col-md-9">
                                    <input type="text" id="outstandtlimit" name="outstandtlimit" class="form-control">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-9 row">
                            <div class="col-md-2">
                                <label class="col-lg-10 control-label">Qty Base</label>
                                <div class="col-md-2">
                                    <input type="radio" id="radqtybase" name="calcmethod" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-lg-10 control-label">Value Base</label>
                                <div class="col-md-2">
                                    <input type="radio" id="radvalbase" name="calcmethod" class="form-control" checked>
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-lg-10 control-label">GP Base</label>
                                <div class="col-md-2">
                                    <input type="radio" id="radgpbase" name="calcmethod" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-lg-10 control-label">Combine ITM</label>
                                <div class="col-md-2">
                                    <input type="checkbox" id="chkcomitm" name="chkcomitm" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <label class="col-lg-3 control-label">Rept.Period</label>
                                <div class="col-md-9">
                                    <input type="text" id="totcommqty" name="totcommqty" class="form-control" value="0">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-9 row">
                            <div class="col-md-2">
                                <label class="col-lg-10 control-label">Invoice</label>
                                <div class="col-md-2">
                                    <input type="radio" id="radinvoice" name="salesmethod" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-lg-10 control-label">Delivered</label>
                                <div class="col-md-2">
                                    <input type="radio" id="radeliverd" name="salesmethod" class="form-control" checked>
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-lg-10 control-label">With Reversal</label>
                                <div class="col-md-2">
                                    <input type="radio" id="radwithreversal" name="salesmethod" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-lg-10 control-label">Active</label>
                                <div class="col-md-2">
                                    <input type="checkbox" id="active" name="active" checked="checked" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-4">
                                <label class="col-lg-3 control-label">Circ No</label>
                                <div class="col-md-9">
                                    <input type="text" id="manualcirno" name="manualcirno" class="form-control">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-9 row">
                            <div class="col-md-4">
                                <label class="col-lg-5 control-label">Con/Max Disc/Rate</label>
                                <div class="col-md-7">
                                    <input type="text" id="maxdiscrate" name="maxdiscrate" value="100" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-lg-10 control-label">PB/PL Include</label>
                                <div class="col-md-2">
                                    <input type="radio" id="withpbpl" name="withpbpl" checked="checked" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-lg-10 control-label">PB/PL Exclude</label>
                                <div class="col-md-2">
                                    <input type="radio" id="withoutpbpl" name="withpbpl" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-lg-10 control-label">Pro.Cir Include</label>
                                <div class="col-md-2">
                                    <input type="radio" id="withprom" name="promo" checked="checked" class="form-control">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-lg-10 control-label">Pro.Cir Exclude</label>
                                <div class="col-md-2">
                                    <input type="radio" id="withoutprom" name="promo" class="form-control">
                                </div>
                            </div>

                        </div>
                    </div>
                    <div class="col-md-9 row">
                        <div class="col-md-6">
                            <div class="col-md-12">
                                <textarea id="remark" name="remark" class="form-control" placeholder="Remark"></textarea>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <label class="col-lg-10 control-label">Re-rep Consider</label>
                            <div class="col-md-2">
                                <input type="radio" id="isrereportall" name="rerep" class="form-control">
                            </div>
                        </div>
                        <div class="col-md-2">
                            <label class="col-lg-10 control-label">Re-rep Month</label>
                            <div class="col-md-2">
                                <input type="radio" id="isrereport" name="rerep" class="form-control">
                            </div>
                        </div>
                        <div class="col-md-2">
                            <label class="col-lg-10 control-label">Re-rep Not Consider</label>
                            <div class="col-md-2">
                                <input type="radio" id="norereport" name="rerep" class="form-control" checked>
                            </div>
                        </div>

                        <div class="col-md-3">
                            <label class="col-lg-3 control-label">From</label>
                            <div class="col-md-9">
                                <input type="text" id="fromdt" name="fromdt" class="form-control" readonly>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <label class="col-lg-3 control-label">Expire</label>
                            <div class="col-md-9">
                                <input type="text" id="date" name="date" class="form-control" readonly>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="panel panel-default row">
            <div class="panel panel-heading">
                Loc/PC Definition
            </div>
            <div class="panel-container">
                <div class="panel panel-body tab-content-cls">
                    <div class="form-horizontal">
                        <div class="col-md-12 row">
                            <div class="col-md-2">
                                <label class="col-md-3 control-label">Channel</label>
                                <div class="col-md-9">
                                    <input type="text" id="chanel" name="chanel" class="form-control with-search">
                                    <img src="~/Resources/Images/Search-icon.png" class="search-img  channel-search">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-md-3 control-label">S/Chnl</label>
                                <div class="col-md-9">
                                    <input type="text" id="schannel" name="schannel" class="form-control with-search">
                                    <img src="~/Resources/Images/Search-icon.png" class="search-img  schannel-search">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-md-3 control-label">Region</label>
                                <div class="col-md-9">
                                    <input type="text" id="region" name="region" class="form-control with-search">
                                    <img src="~/Resources/Images/Search-icon.png" class="search-img  region-search">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-md-3 control-label">Zone</label>
                                <div class="col-md-9">
                                    <input type="text" id="zone" name="zone" class="form-control with-search">
                                    <img src="~/Resources/Images/Search-icon.png" class="search-img  zone-search">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <label class="col-md-3 control-label">PC</label>
                                <div class="col-md-9">
                                    <input type="text" id="pc" name="pc" class="form-control with-search">
                                    <img src="~/Resources/Images/Search-icon.png" class="search-img  pc-search">
                                </div>
                            </div>
                            <div class="col-md-2">
                                <img src="~/Resources/Images/arrow_down.png" class="search-img add-bonus-loc">
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-md-12 form-group item-range-scroll22">
                            <table class="bonus-loc-table table">
                                <tr class="table-hed">
                                    <th>Company</th>
                                    <th>Channel</th>
                                    <th>Sub Chanl</th>
                                    <th>Region</th>
                                    <th>Zone</th>
                                    <th>PC</th>
                                    <th>Remove</th>
                                </tr>
                            </table>
                        </div>
                    </div>

                </div>

            </div>

        </div>
        <div class="panel panel-default row">
            <div class="panel panel-heading">
                Item/Pay Definition
            </div>
            <div class="panel-container">
                <div class="panel panel-body tab-content-cls">
                    <div class="form-horizontal">
                        <div class="col-md-12 row">
