                            <div class="col-sm-8">
                                <input type="text" class="form-control form-control-m temp-name-cre">
                            </div>
                        </div>

                        <div class="template-field-cre">

                        </div>
                    </div>
                    <div class="col-lg-12 bt-set">
                        <button type="button" class="btn btn-primary btn-min float-right cre-add-more-field">Add More Fields</button>
                        <button type="button" class="btn btn-primary btn-min float-right save-newtemp">Save</button>
                        <button type="button" class="btn btn-success btn-min float-right btn-margin-r10 crenew-done">Close</button>
                    </div>
                </div>
            </div>


        </div>
    </div>
</div>

