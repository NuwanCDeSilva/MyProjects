@{
    ViewBag.Title = "Process Arreas";
    var val = System.Web.Configuration.WebConfigurationManager.AppSettings["SiteVersion"].ToString();
}
@section Scripts {
    <script src="~/Scripts/Common/ProcessArreas.js?@val"></script>
}
<div class="op-btns">
    <div class="col-md-6">
        <div class="page-title">Process Arrears</div>
    </div>
    <div class="col-md-6" style="width:540px">
        <input type="button" value="View" class="btn btn-default btn-default-style btn-view-data" />
        <input type="button" value="Process Arrears " class="btn btn-default btn-default-style btn-arrears-process-data" />
    </div>
</div>
<div class="demopage">
    <div class="container1">
        <div class="panel panel-default row">
            <div class="panel panel-heading"></div>
            <div class="panel-container">
                <div class="panel panel-body tab-content-cls">
                    <div class="form-horizontal">
                        <div class="row">
                            <div class="col-md-3">
                                <label class="col-lg-4 control-label">Bonus Month</label>
                                <div class="col-md-8">
                                    <input type="text" id="bmonthdate" name="bmonthdate" class="form-control" readonly>
                                </div>
                            </div>
                            @*<div class="col-md-3">
                                <label class="col-lg-4 control-label">PC</label>
                                <div class="col-md-8">
                                    <input type="text" id="location" name="location" class="form-control with-search" readonly style="background:#eea7a7">
                                    <img src="~/Resources/Images/Search-icon.png" class="search-img  loc-search">
                                </div>
                            </div>*@
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default row">
        <div class="panel panel-heading">Arreas Profit Centers</div>
        <div class="panel-container">
            <div class="panel panel-body tab-content-cls">
                <div class="form-horizontal">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="comm-comm-list-div-scroll">
                                <table class="table table-striped table-hover acc-data-row">
                                    <tr>
                                        <th style="color: black; font-size: x-small">PC</th>
                                        <th style="color: black; font-size: x-small">Arrears Amount</th>
                                        <th style="color: black; font-size: x-small">No Of Arrears Accounts</th>
                                        <th style="color: black; font-size: x-small">Closing Balance</th>
                                        <th style="color: black; font-size: x-small">No Of Act Accounts</th>
                                        <th style="color: black; font-size: x-small"></th>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

