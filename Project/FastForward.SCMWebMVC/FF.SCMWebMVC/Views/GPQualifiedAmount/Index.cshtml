                                </div>*@
                                <div class="col-md-6">
                                    <label class="col-lg-4 control-label">Base</label>
                                    <div class="col-md-8">
                                        @Html.DropDownList("base", new List<SelectListItem> { new SelectListItem { Value = "0", Text = "--Select--" }, new SelectListItem { Value = "Receivable balance", Text = "Receivable balance" } }, null, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <label class="col-lg-4 control-label">Value From</label>
                                    <div class="col-md-8">
                                        <input type="text" id="valuefrom" name="valuefrom" class="form-control with-search">
                                    </div>
                                    
                                </div>
                                <div class="col-md-6">
                                    <label class="col-lg-4 control-label">Rate</label>
                                    <div class="col-md-2">
                                        <input type="checkbox" id="rate" name="rate" checked="checked" class="form-control">
                                    </div>

                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <label class="col-lg-4 control-label">Value To</label>
                                    <div class="col-md-8">
                                        <input type="text" id="valueto" name="valueto" class="form-control with-search">
