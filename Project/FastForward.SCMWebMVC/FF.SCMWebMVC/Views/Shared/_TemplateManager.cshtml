<div class="modal-header header-m">
    <h5 class="modal-title" id="exampleModalLabel">Select Field Template</h5>
</div>
<div class="templatecontent modal-body modal-body-m">
    <div class="col-lg-4"><button type="button" class="btn btn-min btn-success btn-create-template">Create Field Template</button></div>
    <div class="col-lg-8">
        <div class="col-lg-6 float-right no-padding">
            <input class="search-box-m serch-value" placeholder="Search" name="srch-term" id="srch-term" type="text">
            <div class="input-group-btn group-btn-m ">
                <button class="btn btn-default btn-refresh-form"><i class="glyphicon glyphicon-search"></i></button>
            </div>
            <button type="button" class="btn btn-min btn-primary btn-refresh-form  bt2">Refresh</button>
        </div>
        <div class="col-lg-5 float-right text-right ">
            <div class="checkbox-m">
                <label><input type="checkbox" class="with-deleted" value="">With Deleted</label>
            </div>
        </div>
    </div>
    <div class="col-lg-12 table-content">
        <table class="template-det-table table table-bordered m-table">
            <tr class="table-hed">
                <th></th>
                <th class="nodisplay">Id</th>
                <th>Name</th>
                <th>Description</th>
                <th>Create By</th>
                <th>Created Date</th>
                <th></th>
            </tr>
        </table>
    </div>
    <div class="pagination cls-pge-footer margin-none  col-lg-12 pageNavigation-con">
        <div class="right-align cls-pages cls-page-num"><div class="pageNavigation" id="paging-temp"></div></div>
        @*</div>*@
        <div class="page-count cls-pge-cunt">
            <div class="form-group float-right">
                <select class="form-control count-box cls-select-temp" id="sel1">
                    <option value="10">10</option>
                    <option value="25">25</option>
                    <option value="50">50</option>
                    <option value="100">100</option>
                    <option value="500">500</option>
                    <option value="1000">1000</option>
                </select>
            </div>
            <label for="sel1" class="float-right page-count-label">Page Count:</label>
        </div>


    </div>
</div>

<div class="modal-footer modal-footer-n clearfix footer-popup" style="margin-top:0px;padding-top:0px;">
    <div class="input-group add-on full-width">
        <button type="button" class="btn btn-min btn-primary text-right btn-close-tempform">Close</button>
    </div>
</div>
<div class="modal fade main-modal" id="TemplateHdrPop">
    <div class="modal-dialog" role="document">
        <div class="modal-content" style="width:600px; height:340px;">
            <div class="modal-header header-m">
                <h5 class="modal-title"></h5>
            </div>

            <div class="modal-body">
                <div class="col-lg-12">
                    <p class="headding">
                        Please add fields using "Add More Field" button. Fields label can be rename.
                        * If you add values to the added fields, those values will be ignored
                    </p>

                </div>
                <div class="col-lg-12">
                    <div class="content-scroll">
                        <input class="temp-hed-id nodisplay" type="text">
                        <div class="form-group pb-8">
                            <label for="name" class="control-label padding-top-none sec-label pd-left">Template Name</label>
                            <input type="text" class="form-control input-common-width form-control-m temp-name">
                        </div>

                        <div class="template-field">

                        </div>
                    </div>
                    <div class="col-lg-12 bt-set">
                        <button class="btn btn-primary btn-min float-right add-more-field">Add More Fields</button>
                        <button class="btn btn-success btn-min float-right btn-margin-r10 hedpop-done">Done</button>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>



