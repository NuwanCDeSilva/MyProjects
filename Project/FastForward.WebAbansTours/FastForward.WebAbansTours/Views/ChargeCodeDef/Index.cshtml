                                    <div class="col-md-6 form-group">
                                        <label class="col-md-6 control-label">Ticket Details</label>
                                        <div class="col-md-6">
                                            <textarea class="form-control required-field" id="SAC_TIC_DESC" name="SAC_TIC_DESC"></textarea>
                                        </div>
                                    </div>

                                    <div class="col-md-6 form-group">
                                        <label class="col-md-6 control-label">Aditional Details</label>
                                        <div class="col-md-6">
                                            <textarea class="form-control required-field" id="SAC_ADD_DESC" name="SAC_ADD_DESC"></textarea>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6 form-group">
                                        <label class="col-md-6 control-label">From</label>
                                        <div class="col-md-6">
                                            <input type="text" class="form-control with-search required-field" id="SAC_FROM" name="SAC_FROM" />
                                            <img src="~/Resources/Images/Search-icon.png" class="search-img from-town-search">
                                        </div>
                                    </div>
                                    <div class="col-md-6 form-group">
                                        <label class="col-md-6 control-label">Type</label>
                                        <div class="col-md-6">
                                            <input type="text" class="form-control required-field" id="SAC_TP" name="SAC_TP" />
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6 form-group">
                                        <label class="col-md-6 control-label">To</label>
                                        <div class="col-md-6">
                                            <input type="text" class="form-control with-search required-field" id="SAC_TO" name="SAC_TO" />
                                            <img src="~/Resources/Images/Search-icon.png" class="search-img to-town-search">
                                        </div>
                                    </div>
                                    <div class="col-md-6 form-group">
                                        <label class="col-md-6 control-label">Applicable Tax Rate</label>
                                        <div class="col-md-6">
                                            <input type="text" class="form-control" id="SAC_TAX_RT" name="SAC_TAX_RT" placeholder="0.00%" />
                                        </div>
                                    </div>
                                </div>
                                <div class ="row">
                                    <div class="col-md-6 form-group">
                                        <label class="col-md-6 control-label">Ticket Type</label>
                                        <div class="col-md-6">
                                            @Html.DropDownList("SAC_TCKT_TP", new List<SelectListItem>(), "-Select-", new { @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
}
